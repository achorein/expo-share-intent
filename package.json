  "resolutions": {
    "string-width": "^4",
    "strip-ansi": "^4",
    "wrap-ansi": "^7"
  },
  "peerDependencies": {
    "expo": "^53",
    "expo-constants": ">=17.1.5",
    "expo-linking": ">=7.0.2",
    "react": "*",
    "react-native": "*"
