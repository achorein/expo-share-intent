  "resolutions": {
    "string-width": "^4",
    "strip-ansi": "^4",
    "wrap-ansi": "^7"
  },
  "peerDependencies": {
    "expo": "^51",
    "expo-constants": ">=16.0.1",
    "expo-linking": ">=6.3.1",
    "react": "*",
    "react-native": "*"
