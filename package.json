  "devDependencies": {
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.73.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.50.0",
    "expo-module-scripts": "^3.5.1",
    "expo-modules-core": "^1.12.9",
    "prettier": "^3.2.5",
    "release-it": "^17.2.1"
  }
