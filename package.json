    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.1",
    "expo-module-scripts": "^4.0.2",
    "expo-modules-core": "^2.0.1",
    "prettier": "^3.2.5",
    "release-it": "^17.10.0"
  }
